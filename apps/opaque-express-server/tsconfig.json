{
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "./dist"
    },
    "include": ["src"],
    "exclude": ["**/*spec.ts"]
}
